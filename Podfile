  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Ohmicity Backend
	pod 'FirebaseCore'
	pod 'FirebaseAuth'
	pod 'FirebaseDatabase'
	pod 'FirebaseStorage'
	pod 'FirebaseFirestoreSwift'
	pod 'Firebase/Firestore'
	pod 'Firebase/Core'

end
